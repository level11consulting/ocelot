    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Orbital server",
            "type": "lldb",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/target/debug/orb",
            "env": {
                "ORB_DEFAULT_ORG" : "tjtelan",
                "DATABASE_URL": "postgres://orbital:orbital@localhost:5432/orbital",
                "RUST_LOG": "info,orbital_services,orbital_database::postgres::client"
            },
            "args": ["server", "start"],
            "cwd": "${workspaceRoot}",
        }
    ]
}